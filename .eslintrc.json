{
    "extends": "google",
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": false,
      "codeFrame": false,
      "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "no-unused-vars": 0,
        "comma-dangle": 0
    }
}
